PORT=5000
OPENAI_API_KEY=your_openai_api_key_here
# OpenRouter (recommended): OpenAI-compatible API
OPENROUTER_API_KEY=your_openrouter_api_key_here
# Example models:
#   openai/gpt-4o-mini
#   anthropic/claude-3.5-sonnet
#   google/gemini-1.5-pro
OPENROUTER_MODEL=openai/gpt-4o-mini
# Optional but recommended by OpenRouter for attribution
OPENROUTER_SITE_URL=https://your-frontend-domain.com
OPENROUTER_APP_NAME=StoryConnect
# (Optional) If you want to use a custom base URL for OpenAI-compatible providers:
# OPENAI_BASE_URL=https://openrouter.ai/api/v1
JWT_SECRET=your_jwt_secret_here
NODE_ENV=development

# Inbound Apple Health (and other) ingestion
HEALTH_INGEST_TOKEN=change_me_strong_random_token

